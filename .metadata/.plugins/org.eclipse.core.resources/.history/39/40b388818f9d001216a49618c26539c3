package tk.sirtwinkles.spicedtea.world.gen.dungeon.carve.room;

import java.util.ArrayList;

public final class RoomPopulator {
	
	public static void populateRooms(int level, ArrayList<Room> rooms, int[][] data) {
		for (Room r : rooms) {
			if (r.feature != null) {
				RoomFeatureFactory.getFeature(r.feature).generate(r, data);
			}
		}
	}

	private RoomPopulator() {} //Never instantiate.
}
