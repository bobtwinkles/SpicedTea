package tk.sirtwinkles.spicedtea.input;

public class TouchEvent {
	private int x, y, pointer;
	
	public TouchEvent(int x, int y, int pointer) {
		this.x = x;
		this.y = y;
		this.pointer = pointer;
	}

	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}

	public int getPointer() {
		return pointer;
	}
}
